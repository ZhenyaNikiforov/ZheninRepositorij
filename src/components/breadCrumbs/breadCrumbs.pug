mixin breadCrumbs(options= {})
  -
    const {
      href1= '',
      refer1= '',
      href2= '',
      refer2= ''
    }= options;
  nav.container_hlebnyikh_kroshek
    ul.hlebnyie_kroshki
      li#pervaya_kroshka.hlebnaya_kroshka
        a.ssyilka_hlebnoj_kroshki(href=href1)= refer1
      li.hlebnaya_kroshka
        a.ssyilka_hlebnoj_kroshki(href=href2)= refer2